<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    android:orientation="vertical" >

    <include layout="@layout/top_title_layout" />

    <LinearLayout 
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:orientation="vertical">

    <TextView
        android:id="@+id/personClear"
        style="@style/personOtherItemStyle"
        android:text="清除缓存" />
     <TextView
        android:id="@+id/personContranct"
        style="@style/personOtherItemStyle"
        android:text="使用服务条款" />
      <TextView
        android:id="@+id/personAbout"
        style="@style/personOtherItemStyle"
        android:text="关于我们" />
       <TextView
        android:id="@+id/personFeedBack"
        style="@style/personOtherItemStyle"
        android:text="意见反馈" />
    </LinearLayout>

    
    <TextView
        android:id="@+id/personLoginOut"
        style="@style/personOtherItemStyle"
        android:background="@drawable/white_btn_bg"
        android:textColor="@color/style_color"
        android:layout_marginTop="40dp"
        android:visibility="gone"
        android:text="退出登录" />
</LinearLayout>